%p in users/index

%table
	%tr 
		%td= 'id' 
		%td='username'
		%td='email'
		%td='password'
	-@users.each do |user|
		%tr 
			%td=user.id
			%td=user.username
			%td=user.email
			%td=user.password
			%td=link_to 'delete user', user_path(user), method: :delete, data: { confirm: 'Are you sure?' }

=link_to 'home', root_path	